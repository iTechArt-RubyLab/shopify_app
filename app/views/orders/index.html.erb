<h1>All Orders</h1>

<table>
  <thead>
  <tr>
    <th>Order ID</th>
    <th>Name</th>
    <th>Total Price</th>
    <th>Created At</th>
    <th>Updated At</th>
  </tr>
  </thead>
  <tbody>
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.id %></td>
      <td><%= link_to order.name, order_path(order) %></td>
      <td><%= order.total_price %></td>
      <td><%= order.created_at %></td>
      <td><%= order.updated_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>
